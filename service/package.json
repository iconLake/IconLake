{
  "name": "service",
  "version": "1.0.0",
  "description": "iconLake Service",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "dev:server": "pnpm exec pm2 reload ecosystem.config.cjs && pnpm exec pm2 logs service-dev",
    "dev:front": "pnpm exec gulp",
    "dev": "pnpm i && pnpm exec npm-run-all -p dev:*",
    "dev-clean": "pnpm exec pm2 delete service-dev",
    "pro": "pnpm i -P && pm2 reload ecosystem.config.cjs --env production",
    "test": "pnpm i -P && pm2 reload ecosystem.config.cjs --env test"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "packageManager": "pnpm@7.14.0",
  "author": "iconLake",
  "license": "AGPLv3",
  "dependencies": {
    "axios": "^0.27.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cos-nodejs-sdk-v5": "^2.11.13",
    "cron": "^2.1.0",
    "csso": "^5.0.5",
    "deep-eql": "^4.1.1",
    "express": "^4.18.2",
    "filter-obj": "^5.1.0",
    "helmet": "^6.0.0",
    "mongoose": "^6.6.5",
    "nanoid": "^4.0.0",
    "node-fetch": "^3.2.10",
    "uglify-js": "^3.17.3",
    "webfont": "^11.2.26"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^8.25.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-uglify": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "pm2": "^5.2.2",
    "react": "^18.2.0",
    "sass": "^1.55.0",
    "typescript": "^4.8.4"
  }
}
