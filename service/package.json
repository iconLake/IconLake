{
  "name": "service",
  "version": "1.0.0",
  "description": "iconLake Service",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "dev:server": "pnpm exec pm2 reload ecosystem.config.cjs && pnpm exec pm2 logs service-dev",
    "dev:front": "pnpm exec gulp",
    "dev": "pnpm i && pnpm exec npm-run-all -p dev:*",
    "dev-clean": "pnpm exec pm2 delete service-dev",
    "build:server": "pnpm i -P=false && pnpm exec gulp build",
    "build:www": "cd ../www && pnpm i -P=false && pnpm run build-only",
    "build": "pnpm i -P=false && pnpm exec npm-run-all -p build:*",
    "pro": "pnpm i -P && pm2 reload ecosystem.config.cjs --env production",
    "test": "pnpm i -P && pm2 reload ecosystem.config.cjs --env test"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "packageManager": "pnpm@9.4.0",
  "author": "iconLake",
  "license": "AGPLv3",
  "dependencies": {
    "@cosmjs/amino": "^0.30.1",
    "@cosmjs/crypto": "^0.30.1",
    "@cosmjs/encoding": "^0.30.1",
    "@cosmjs/proto-signing": "^0.31.0",
    "@iconlake/client": "^0.1.10",
    "@keplr-wallet/cosmos": "^0.11.63",
    "axios": "^0.27.2",
    "clean-css": "^5.3.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cos-nodejs-sdk-v5": "^2.12.1",
    "cron": "^2.3.0",
    "deep-eql": "^4.1.3",
    "express": "^4.18.2",
    "filter-obj": "^5.1.0",
    "helmet": "^6.2.0",
    "isbot": "^3.7.0",
    "js-sha256": "^0.10.1",
    "mongoose": "^6.11.1",
    "nanoid": "^4.0.2",
    "node-fetch": "^3.3.1",
    "uglify-js": "^3.17.4",
    "webfont": "^11.2.26"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/node": "^20.2.3",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.41.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-json-minify": "^1.2.3",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.4",
    "gulp-sass": "^5.1.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-uglify": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "pm2": "^5.3.0",
    "react": "^18.2.0",
    "sass": "^1.62.1",
    "typescript": "^4.9.5"
  }
}
